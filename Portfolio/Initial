CREATE TABLE IF NOT EXISTS `__EFMigrationsHistory` (
    `MigrationId` varchar(150) CHARACTER SET utf8mb4 NOT NULL,
    `ProductVersion` varchar(32) CHARACTER SET utf8mb4 NOT NULL,
    CONSTRAINT `PK___EFMigrationsHistory` PRIMARY KEY (`MigrationId`)
) CHARACTER SET=utf8mb4;

START TRANSACTION;

ALTER DATABASE CHARACTER SET utf8mb4;

CREATE TABLE `ContactMessages` (
    `Id` int NOT NULL AUTO_INCREMENT,
    `FromEmail` longtext CHARACTER SET utf8mb4 NOT NULL,
    `FromName` longtext CHARACTER SET utf8mb4 NOT NULL,
    `CreatedOn` datetime(6) NOT NULL,
    `Subject` longtext CHARACTER SET utf8mb4 NULL,
    `Body` longtext CHARACTER SET utf8mb4 NOT NULL,
    CONSTRAINT `PK_ContactMessages` PRIMARY KEY (`Id`)
) CHARACTER SET=utf8mb4;

INSERT INTO `__EFMigrationsHistory` (`MigrationId`, `ProductVersion`)
VALUES ('20231228042512_Initial', '8.0.0');

COMMIT;

